{
  "name": "productivity-hub-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 13000",
    "dev:turbo": "next dev --turbopack --port 13000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:ui": "playwright test --ui",
    "e2e:report": "playwright show-report",
    "e2e:install": "playwright install",
    "test:ci": "npm run test && npm run e2e",
    "test:routes": "node route-coverage-test.js",
    "test:routes:dev": "node dev-route-test.js",
    "test:routes:prod": "node test-with-build.js",
    "test:performance": "node performance-test.js",
    "test:visual": "node visual-testing-agent.js",
    "test:visual:baseline": "node visual-testing-agent.js --mode baseline",
    "test:visual:compare": "node visual-testing-agent.js --mode compare",
    "test:visual:report": "node visual-testing-agent.js --mode report",
    "test:visual:clean": "rm -rf visual-test-results screenshots baselines diffs",
    "test:visual:simple": "node simple-visual-test.js",
    "test:visual:simple:compare": "node simple-visual-test.js --compare",
    "test:all": "npm run test:routes:dev && npm run test:visual:simple",
    "type-check": "tsc --noEmit"
  },
